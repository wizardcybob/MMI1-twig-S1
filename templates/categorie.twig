{% extends "base.twig" %}

{# Ce modèle TWIG n'a pas de contenu, il crée deux blocs (child_main et aside) vides dans le bloc main.
Ces deux blocs sont spécialisés dans les modèles fils #}


{% block ccs_page %}
	<link rel="stylesheet" href="css/categorie.css">
	<link rel="stylesheet" href="css/card_effect.css">
{% endblock %}


{% block main %}
	<section class="main">		
		<div class="wrap_categorie">
			{# BLOC 1 -> child_main #}
			{% block child_main %}

			{% endblock child_main %}

			{# BLOC 2 -> aside #}
			{% block aside %}
		
			{% endblock aside %}
		</div>
	</section>

{% endblock main %}


{% block javascript %}
	<script src="./js/aside.js"></script>
	<script src="./js/card_effect.js"></script>
{% endblock %}
